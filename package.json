{
  "name": "experts",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.8.0",
  "workspaces": [
    "domains/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "postbuild": "next-sitemap",
    "sitemap:all": "node generate-sitemaps.js",
    "dev:office": "turbo run dev --filter=@experts/office",
    "dev:word": "turbo run dev --filter=@experts/word",
    "dev:excel": "turbo run dev --filter=@experts/excel",
    "dev:access": "turbo run dev --filter=@experts/access",
    "dev:powerplatform": "turbo run dev --filter=@experts/powerplatform",
    "scan": "node node-scripts/scan-codebase.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.4",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "docx": "^9.0.2",
    "exceljs": "^4.4.0",
    "fs-extra": "^11.2.0",
    "latest-version": "^9.0.0",
    "next": "14.2.15",
    "node-fetch": "^3.3.2",
    "puppeteer": "^23.7.0",
    "react": "^18",
    "react-dom": "^18",
    "sass": "^1.80.6",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "eslint-plugin-react": "^7.37.1",
    "globals": "^15.11.0",
    "turbo": "latest"
  },
  "type": "module"
}
